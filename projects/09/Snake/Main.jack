class Main {
    function void main(){
        var Game game;
        let game = Main.initialSetup();
        do game.run();
        return;
    }

    function Game initialSetup(){
        var Pellets pellets;
        var Snake snake;
        var Canvas canvas;
        var Game game;

        let pellets = Pellets.new(8848);
        let snake = Snake.new();
        let canvas = Canvas.new(snake, pellets);
        let game = Game.new(canvas);
        return game;
    }
}
